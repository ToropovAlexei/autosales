  "query": "\n            SELECT\n                sm.id,\n                sm.order_id,\n                sm.product_id,\n                sm.type as \"type: _\",\n                sm.quantity,\n                sm.created_by,\n                sm.description,\n                sm.reference_id,\n                sm.balance_after,\n                sm.created_at,\n                p.name AS product_name\n            FROM stock_movements sm\n            LEFT JOIN products p ON sm.product_id = p.id\n            WHERE sm.id = $1\n            ",
